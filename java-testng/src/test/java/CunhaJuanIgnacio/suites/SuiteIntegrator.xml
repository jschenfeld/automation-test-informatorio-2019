<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Suite integrator" verbose="1" parallel="methods"
	thread-count="1">
	<groups>
		<run>
			<include name="integrator" />
		</run>
	</groups>
	
	<test name="ChromeTesting">
		<parameter name="browser" value="CHROME" />
		
		<classes>
			<class name="CunhaJuanIgnacio.AutomationPractice" />
		</classes>
	</test>

	<test name="FirefoxTesting">
		<parameter name="browser" value="FIREFOX" />
		<classes>
			<class name="CunhaJuanIgnacio.AutomationPractice" />
		</classes>
	</test>

</suite>